{
    "name": "eslint-prettier",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "type": "commonjs",
    "scripts": {
        "start": "node src/server.js",
        "test": "echo \"Error: no test specified\" && exit 1",
        "prepare": "husky install"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "**/*.{js,jsx,ts,tsx}": [
            "npx prettier src/**/*.js --write",
            "npx eslint src/**/*.js --fix"
        ]
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "dotenv": "^16.3.1",
        "express": "^4.18.2"
    },
    "devDependencies": {
        "eslint": "^8.46.0",
        "husky": "^8.0.0",
        "lint-staged": "^13.2.3",
        "prettier": "^3.0.0"
    }
}
